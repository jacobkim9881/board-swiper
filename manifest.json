{
  "name": "Board Swiper",
  "version": "0.5.0.0",
  "default_locale": "en",
  "description": "An extension helping user go to next or previous page on a board",
  "icons": {
  "16": "icon/icon16.png",
  "32": "icon/icon32.png",
  "64": "icon/icon64.png"
},
  "author": "jacobkim9881",
  "commands": {
    "previous": {
      "suggested_key": {
	    "default": "Ctrl+Right"
      },
      "description": "previous"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
	  {
	  "matches": [ "<all_urls>"],
	  "js": ["content.js"]
  }
  ],
  "permissions": ["storage", "tabs"],
  "manifest_version": 3
}

